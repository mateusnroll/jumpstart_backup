{
	"name": "Jumpstart 2",
	"description": "Get quickly to everything you need",
	"version": "0.1",
	"manifest_version": 2,

	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

	"chrome_url_overrides": {
		"newtab": "index.html"
	},

	"permissions": [
		"storage",
		"bookmarks",
		"chrome://favicon/*"
	]
}